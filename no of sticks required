#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    int a,b,c=0;
	    cin>>a>>b;
	    int l=a+b;
	    int ay[10] = {6,2,5,5,4,5,6,3,7,6};
	    while(l)
	    {
	        int k= l%10;
	        c=c+ay[k];
	        l=l/10;
	        
	    }
	    cout<<c<<endl;
	}
	return 0;
}
