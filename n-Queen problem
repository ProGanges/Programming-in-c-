#include <iostream>
#include<cmath>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    double n;
	    cin>>n;
	    double x = pow(0.143*n,n);
	    double y = floor(x);
	    if(x-y < 0.5)
	    {
	        cout<<y<<endl;
	    }
	    else{
	        cout<<y+1<<endl;
	    }
	}
	return 0;
}
