#include <iostream>
using namespace std;
void result(int l,int m)
{
    if(m>l)
    {
        cout<<"S"<<endl;
    }
    else
    {
        cout<<"N"<<endl;
    }
}
int satyarth(int x,int y,int z)
{
    if(y>x)
    {
     x=x+z;  
    }
    else
    {
        y=y+z;
    }
    result(x,y);
    
    
    return 0;
}


int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    int a,b,c,d;
	    cin>>a>>b>>c>>d;
	  
	   if(b>a)
	   {
	       a=a+c;
	   }
	   else 
	   {
	       b=b+c;
	   }
	  satyarth(a,b,d);
	   
	}
	return 0;
}
