#include <iostream>
#include<cstring>
#include<string.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	   int n;
	   cin>>n;
	   int k = 0, c =0;
	  string s[n];
	  string s1 ="START38", s2 = "LTIME108";
	   for(int i=0;i<n;i++)
	   {
	      cin>>s[i];
	   }
	   for(int i=0;i<n;i++)
	   {
	      if(s[i]==s1)
	      {
	          c++;
	      }
	      else if(s[i]==s2)
	      {
	          k++;
	      }
	      
	   }
	   cout<<c<<" "<<k<<endl;
	   
	}
	return 0;
}
